    "@types/angular-mocks": "1.5.9",
    "@types/angular-route": "1.3.4",
    "@types/jasmine": "^2.8.6",
    "jasmine-core": "^2.6.2",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "3.0.12",
